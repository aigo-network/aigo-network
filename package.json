{
  "name": "aigo",
  "version": "0.0.2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "codegen": "graphql-codegen --config tools/codegen.ts",
    "test": "jest",
    "deploy:prod": "sst deploy --stage production",
    "deploy:staging": "sst deploy --stage staging",
    "deploy:dev": "sst deploy --stage development",
    "deploy": "sst deploy"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@metacraft/eslint-config": "^0.1.3",
    "@metacraft/tsconfig": "^0.0.2",
    "@types/mustache": "^4",
    "eslint": "^8.19.0",
    "graphql": "^16.8.1",
    "graphql-request": "6.1.0",
    "jest": "^29.6.3",
    "sst": "^2.43.2",
    "typescript": "5.4.5",
    "webpack": "^5.92.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "mustache": "^4.2.0"
  }
